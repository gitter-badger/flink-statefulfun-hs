{
  "nixEnvSelector.nixShellConfig": "${workspaceRoot}/shell.nix",
  "files.watcherExclude": {
    "**/target": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/dist-newstyle": true
  },
  "terminal.integrated.shell.linux": "nix-shell"
}